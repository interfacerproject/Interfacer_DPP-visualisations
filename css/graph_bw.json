[
    {
        "selector": "node",
        "style": {
            "font-size": "20px",
            "font-style": "normal",
            "text-wrap": "wrap",
            "text-justification": "center",
            "text-max-width": "1075px",
            "font-family": "FontAwesome, Arial, Helvetica, sans-serif",
            "width": 40,
            "height": 40,
            "label": "Node"
        }
    },
    {
        "selector": "node[type = 'EconomicResource'][?origin]",
        "style": {
            "background-color": "#f7f7f7",
            "shape": "ellipse"
        }
    },
    {
        "selector": "node[type = 'EconomicResource'][!origin]",
        "style": {
            "background-color": "#f7f7f7",
            "shape": "ellipse"
        }
    },
    {
        "selector": "node[type = 'EconomicEvent']",
        "style": {
            "background-color": "#cccccc",
            "shape": "rectangle"
        }
    },
    {
        "selector": "node[type = 'EconomicEvent'][name = 'transfer']",
        "style": {
            "background-color": "#969696"
        }
    },
    {
        "selector": "node[type = 'EconomicEvent'][name = 'transferCustody']",
        "style": {
            "background-color": "#969696"
        }
    },
    {
        "selector": "node[type = 'EconomicEvent'][name = 'transferAllRights']",
        "style": {
            "background-color": "#969696"
        }
    },
    {
        "selector": "node[type = 'Process']",
        "style": {
            "background-color": "#525252",
            "shape": "diamond"
        }
    },
    {
        "selector": "node[type = 'ProcessGroup']",
        "style": {
            "background-color": "#21897E",
            "shape": "diamond",
            "opacity": 0.8
        }
    },
    {
        "selector": "node[type = 'Person']",
        "style": {
            "background-color": "#000000",
            "shape": "triangle"
        }
    },
    {
        "selector": "edge",
        "style": {
            "width": 1,
            "font-size": "10px",
            "font-style": "oblique",
            "line-color": "#969696",
            "target-arrow-color": "#969696",
            "target-arrow-shape": "triangle",
            "curve-style": "bezier"
        }
    },
    {
        "selector": "[?grouped]",
        "style": {
            "opacity": 0.5
        }
    }
